A,GAAA,cAAA,KAAA,CAAA,WAAA,EAAA;AACA,KA3BA;AA4BA,IAAA,WA5BA,yBA4BA;AACA,aAAA,KAAA,QAAA,IAAA,CAAA,KAAA,IAAA,CAAA,OAAA;AACA,KA9BA;AAgCA,IAAA,OAhCA,mBAgCA,GAhCA,EAgCA;AACA;AACA,WAAA,KAAA,GAAA,QAAA,CAAA,GAAA,CAAA;AACA,KAnCA;AAqCA,IAAA,QArCA,oBAqCA,CArCA,EAqCA;AACA,UAAA,CAAA,KAAA,KAAA,KAAA,EAAA;AACA,aAAA,EAAA,GAAA,CAAA;AACA,OAFA,MAEA;AACA,aAAA,EAAA,GAAA,CAAA,GAAA,CAAA;AACA;AACA;AA3CA,GApDA;AAiGA,EAAA,OAjGA,qBAiGA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,SAAA,EAAA,KAAA,IAAA;AACA;AAnGA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-stepper alt-labels v-model=\"e1\">\n      <v-stepper-header>\n        <v-stepper-step step=\"1\">Dados Pessoais</v-stepper-step>\n\n        <!-- <v-divider></v-divider> -->\n\n        <v-stepper-step step=\"2\">Endereço</v-stepper-step>\n\n        <!-- <v-divider></v-divider> -->\n\n        <v-stepper-step step=\"3\">Dados da conta</v-stepper-step>\n      </v-stepper-header>\n      <v-stepper-items>\n        <v-stepper-content step=\"1\">\n          <!-- TODO imagem -->\n          <v-form ref=\"steponedata\">\n            <v-text-field\n              :disabled=\"disapleForm()\"\n              outlined\n              label=\"Nome\"\n              :rules=\"[$vln.requiredRule('Nome'), $vln.moreThanRule(6)]\"\n              required\n              v-model=\"nome\"\n            ></v-text-field>\n            <v-text-field\n              :disabled=\"disapleForm()\"\n              outlined\n              label=\"CPF\"\n              required\n              v-model=\"cpf\"\n              v-mask=\"cpfMask\"\n              :rules=\"[$vln.requiredRule('CPF'), $vln.cpflRule()]\"\n            ></v-text-field>\n            <!-- date picker -->\n            <v-menu\n              ref=\"dpnascimento\"\n              v-model=\"datanascimentomenu\"\n              :close-on-content-click=\"false\"\n              transition=\"scale-transition\"\n              offset-y\n              min-width=\"290px\"\n            >\n              <template v-slot:activator=\"{ on }\">\n                <v-text-field\n                  :disabled=\"disapleForm()\"\n                  v-model=\"datanascimento\"\n                  label=\"Data de nascimento\"\n                  readonly\n                  v-on=\"on\"\n                  clearable\n                  outlined\n                  :rules=\"[$vln.requiredRule('Data de nascimento')]\"\n                  @click:clear=\"datanascimento = null\"\n                ></v-text-field>\n              </template>\n              <v-date-picker\n                ref=\"picker\"\n                v-model=\"datanascimento\"\n                :max=\"new Date().toISOString().substr(0, 10)\"\n                min=\"1950-01-01\"\n                @change=\"saveDate\"\n              ></v-date-picker>\n            </v-menu>\n            <!-- date picker -->\n            <v-text-field\n              :disabled=\"disapleForm()\"\n              outlined\n              label=\"Telefone para contato\"\n              required\n              v-model=\"telefone\"\n              v-mask=\"'(##) #####-####'\"\n              :rules=\"[$vln.requiredRule('Telefone'),$vln.foneRule(11)]\"\n            ></v-text-field>\n            <span class=\"mb-0 grey--text text--darken-2\">O número informado tem Whatsapp?</span>\n            <v-switch\n            :disabled=\"disapleForm()\"\n            v-model=\"whatsapp\"\n            class=\"ma-4\" :label=\"whatsapp ? 'Sim' : 'Não'\"></v-switch>\n\n            <v-row justify=\"space-around\">\n              <v-col>\n                <span class=\"mb-0 grey--text text--darken-2\">Mora sozinho?</span>\n                <v-switch\n                :disabled=\"disapleForm()\"\n                v-model=\"moraso\"\n                class=\"ma-4\" :label=\"moraso ? 'Sim' : 'Não'\"></v-switch>\n              </v-col>\n              <v-col>\n                <span class=\"mb-0 grey--text text--darken-2\">É grupo de risco?</span>\n                <v-switch\n                  :disabled=\"disapleForm()\"\n                  v-model=\"grupoderisco\"\n                  class=\"ma-4\"\n                  :label=\"grupoderisco ? 'Sim' : 'Não'\"></v-switch>\n              </v-col>\n            </v-row>\n            <v-btn\n              block\n              rounded\n              x-large\n              color=\"primary\"\n              @click=\"stepOneClick()\"\n              :loading=\"user.loading\">Próximo</v-btn>\n          </v-form>\n        </v-stepper-content>\n        <v-stepper-content step=\"2\">\n          <v-form ref=\"steptwodata\">\n            <v-text-field\n              :disabled=\"disapleForm()\"\n              outlined\n              label=\"Cep\"\n              v-mask=\"cepMask\"\n              :rules=\"[$vln.requiredRule('Cep')]\"\n              required\n              v-model=\"cep\"\n            ></v-text-field>\n            <v-text-field\n              :disabled=\"disapleForm()\"\n              outlined\n              label=\"Endereço\"\n              :rules=\"[$vln.requiredRule('Endereço')]\"\n              required\n              v-model=\"endereco\"\n            ></v-text-field>\n            <v-text-field\n              :disabled=\"disapleForm()\"\n              outlined\n              label=\"Bairro\"\n              :rules=\"[$vln.requiredRule('Bairro')]\"\n              required\n              v-model=\"bairro\"\n            ></v-text-field>\n            <v-text-field\n              :disabled=\"disapleForm()\"\n              outlined\n              label=\"Cidade\"\n              :rules=\"[$vln.requiredRule('Cidade')]\"\n              required\n              v-model=\"cidade\"\n            ></v-text-field>\n            <v-text-field\n              :disabled=\"disapleForm()\"\n              outlined\n              label=\"Estado\"\n              :rules=\"[$vln.requiredRule('Estado')]\"\n              required\n              v-model=\"estado\"\n            ></v-text-field>\n            <v-btn\n              block\n              rounded\n              x-large\n              color=\"primary\"\n              @click=\"stepTwoClick()\"\n              :loading=\"user.loading\">Próximo</v-btn>\n          </v-form>\n        </v-stepper-content>\n        <v-stepper-content step=\"3\">\n          <v-form ref=\"stepthreedata\">\n            <v-text-field\n              :disabled=\"disapleForm()\"\n              outlined\n              label=\"Email\"\n              required\n              :rules=\"[$vln.requiredRule('E-mail'), $vln.emailRule()]\"\n              v-model=\"email\"\n              ></v-text-field>\n            <v-text-field\n              :disabled=\"disapleForm()\"\n              outlined\n              label=\"Senha\"\n              required\n              type=\"password\"\n              :rules=\"[$vln.requiredRule('Senha'), $vln.moreThanRule(6)]\"\n              v-model=\"password\"\n              ></v-text-field>\n            <v-text-field\n              :disabled=\"disapleForm()\"\n              outlined\n              label=\"Repetir Senha\"\n              required\n              type=\"password\"\n              :rules=\"[$vln.requiredRule('Repitir Senha'), $vln.mustBeEqualPass(password),]\"\n              v-model=\"repassword\"\n              ></v-text-field>\n              <v-checkbox\n                :disabled=\"disapleForm()\"\n                v-model=\"checkbox\"\n                :rules=\"[$vln.requiredRule('Ler o contrato')]\"\n                label=\"Li e aceito os termos de uso do Conta Comigo app\"\n              ></v-checkbox>\n            <v-btn\n              block\n              rounded\n              x-large\n              color=\"primary\"\n              @click=\"stepThreeClick()\"\n              :loading=\"user.loading\">Registrar-se</v-btn>\n          </v-form>\n        </v-stepper-content>\n      </v-stepper-items>\n    </v-stepper>\n\n    <!-- <v-form ref=\"userform\" class=\"ma-5\" >\n        <v-radio-group\n          :rules=\"[$vln.requiredRule('Sexo')]\"\n          :disabled=\"disapleForm()\"\n          v-model=\"sexo\"\n          label=\"Sexo:\"\n          class=\"mb-3\"\n          row>\n          <v-radio\n            label=\"Masculino\"\n            value=\"m\"\n          ></v-radio>\n          <v-radio\n            label=\"Feminino\"\n            value=\"f\"\n          ></v-radio>\n          <v-radio\n            label=\"Não sei\"\n            value=\"n/s\"\n          ></v-radio>\n        </v-radio-group>\n      <v-btn\n        block\n        rounded\n        x-large\n        color=\"primary\"\n        @click=\"createAccount()\"\n        :loading=\"user.loading\">registrar Usuário</v-btn>\n      <p class=\"mt-4 red--text text-center\" v-if=\"user.loginError\">{{user.loginError}}</p>\n    </v-form> -->\n  </div>\n</template>\n<script>\n\nimport { mapState } from 'vuex';\n\nexport default {\n  props: ['editavel'],\n  computed: mapState(['user']),\n  watch: {\n    datanascimentomenu(val) {\n      // eslint-disable-next-line no-unused-expressions\n      val && setTimeout(() => {\n        this.$refs.picker.activePicker = 'YEAR';\n      });\n    }, // stepper\n    steps(val) {\n      if (this.e1 > val) {\n        this.e1 = val;\n      }\n    },\n    vertical() {\n      this.e1 = 2;\n      requestAnimationFrame(() => { this.e1 = 1; }); // Workarounds\n    },\n  },\n  data() {\n    return {\n      nome: '',\n      cpf: '',\n      cpfMask: '###.###.###-##',\n      datanascimento: null,\n      datanascimentomenu: false,\n      telefone: '',\n      whatsapp: false,\n      moraso: false,\n      grupoderisco: false,\n\n      cep: '',\n      cepMask: '#####-###',\n      endereco: '',\n      bairro: '',\n      cidade: '',\n      estado: '',\n\n      username: '',\n      email: '',\n      password: '',\n      repassword: '',\n      checkbox: false,\n      // stepper\n      e1: 1,\n      steps: 3,\n      vertical: false,\n      altLabels: false,\n      editable: true,\n    };\n  },\n  methods: {\n    stepOneClick() {\n      if (this.$refs.steponedata.validate()) {\n        this.e1 = 2;\n      }\n    },\n    stepTwoClick() {\n      if (this.$refs.steptwodata.validate()) {\n        this.e1 = 3;\n      }\n    },\n    stepThreeClick() {\n      console.log('stepThreeClick()', this.$refs.stepthreedata.validate());\n      // this.e1 = 2;\n    },\n\n    createAccount() {\n      console.log('createAccount', this.$refs.userformum.validate());\n      if (this.$refs.userformum.validate()) {\n        // this.$store.dispatch('user/register', { email: this.email, password: this.password });\n      }\n    },\n    saveDate(date) {\n      const mdate = new Date(date);\n      const dia = mdate.getDay() < 0 ? `0${mdate.getDay()}` : mdate.getDay();\n      const mes = (mdate.getMonth() + 1) < 0 ? `0${(mdate.getMonth() + 1)}` : (mdate.getMonth() + 1);\n      this.$refs.dpnascimento.save(`${dia}-${mes}-${mdate.getFullYear()}`);\n    },\n    disapleForm() {\n      return this.editavel && !this.user.loading;\n    },\n\n    onInput(val) {\n      // eslint-disable-next-line radix\n      this.steps = parseInt(val);\n    },\n\n    nextStep(n) {\n      if (n === this.steps) {\n        this.e1 = 1;\n      } else {\n        this.e1 = n + 1;\n      }\n    },\n  },\n  created() {\n    console.log('created', this.$vln);\n  },\n};\n</script>\n<style lang=\"scss\" scoped>\n.v-stepper{\n  box-shadow: none;\n  &__header{\n    box-shadow: none;\n  }\n  &__step {\n    flex-basis: 0;\n  }\n}\n</style>\n"],"sourceRoot":"src/components"}]}